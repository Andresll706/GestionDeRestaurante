<Page x:Class="PAEE_FINAL.PageHome"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:PAEE_FINAL"
      xmlns:p="clr-namespace:PAEE_FINAL.Properties"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PageHome">

    <Grid>
        <Label x:Name="title" Style="{StaticResource Titles}" Content="{x:Static p:Resources.titleHome}" VerticalAlignment="Top" HorizontalAlignment="Center" Margin="10,19,0,0"></Label>
        <TextBlock x:Name="subtitle"   TextWrapping="Wrap"  Foreground="Black" FontSize="18"  Text="{x:Static p:Resources.subTitleHome}"  HorizontalAlignment="Center" VerticalAlignment="Top" Margin="186.5,80,185.5,0" Width="428" Panel.ZIndex="-5" Height="72" UseLayoutRounding="False" ></TextBlock>
        <TextBlock x:Name="subtitle2"   TextWrapping="Wrap"  Foreground="Black" FontSize="18" Text="{x:Static p:Resources.subTitle2Home}" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="231,166,244,0" Width="325" />
    </Grid>
</Page>
